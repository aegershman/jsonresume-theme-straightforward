  "license": " GPL-3.0",
  "author": "Aaron Gershman",
  "keywords": [
    "jsonresume",
    "theme",
    "straightforward"
  ],
  "files": [
    "index.js",
    "partials/*.hbs",
    "resume.hbs",
    "style.css"
  ],
  "scripts": {

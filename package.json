{
  "name": "jsonresume-theme-straightforward",
  "version": "0.0.0-development",
  "description": "a straightforward jsonresume theme",
  "homepage": "https://github.com/aegershman/jsonresume-theme-straightforward",
  "bugs": {
    "url": "https://github.com/aegershman/jsonresume-theme-straightforward/issues"
  },
  "repository": {
